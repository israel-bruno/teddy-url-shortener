{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/teddy-url-shortener/src",
  "monorepo": true,
  "root": "apps/teddy-url-shortener",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/teddy-url-shortener/tsconfig.app.json"
  },
  "projects": {
    "urls": {
      "type": "application",
      "root": "apps/urls",
      "entryFile": "main",
      "sourceRoot": "apps/urls/src",
      "compilerOptions": {
        "tsConfigPath": "apps/urls/tsconfig.app.json"
      }
    }
  }
}